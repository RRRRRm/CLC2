{
  "name": "clc2",
  "productName": "clc2",
  "version": "0.1.0",
  "description": "Calculus-ator, reimagined",
  "main": "./electron/main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=electron/style/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=electron/style/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"CLC2\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=electron/style/icon.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/clc2-darwin-x64/clc2.app clc2 --out=release-builds --overwrite --icon=electron/style/icon.icns"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheCatster/CLC2.git"
  },
  "author": "White Hat Cat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TheCatster/CLC2/issues"
  },
  "homepage": "https://github.com/TheCatster/CLC2#readme",
  "devDependencies": {
    "electron": "^8.2.5",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "algebrite": "^1.3.1",
    "katex": "^0.11.1"
  }
}
